{
    "multipart": [
        {   "apply": { "model": "mechanimation:panel/steel/post_ends" }},
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "mechanimation:panel/steel/post" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "mechanimation:panel/steel/cap" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "mechanimation:panel/steel/cap", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "mechanimation:panel/steel/cap_alt" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "mechanimation:panel/steel/cap_alt", "y": 90 }
        },
        {   "when": { "north": true },
            "apply": { "model": "mechanimation:panel/steel/side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "mechanimation:panel/steel/side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "mechanimation:panel/steel/side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "mechanimation:panel/steel/side_alt", "y": 90 }
        }
    ]
}
